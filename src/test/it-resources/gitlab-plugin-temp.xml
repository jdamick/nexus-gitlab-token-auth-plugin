<?xml version="1.0" encoding="UTF-8"?>
<gitlabConfiguration>
  <gitlabServerUrl>GITLAB_URL</gitlabServerUrl>
  <defaultRoles>
    <defaultRole>person</defaultRole>
  </defaultRoles>
  <adminRoles>
    <adminRole>nx-admin</adminRole>
  </adminRoles>
  <cacheValidationInterval>0</cacheValidationInterval>
  <userIdProvider>ldap</userIdProvider>
  <userIdField>extern_uid</userIdField>
  <userIdFieldMatch>uid=(\w*).*</userIdFieldMatch>
</gitlabConfiguration>
